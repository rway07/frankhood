        "laravel-mix": "^5.0.1",
        "lodash": "4.17.21",
        "resolve-url-loader": "^3.1.0",
        "stylelint": "^16.9.0",
        "stylelint-config-standard": "^36.0.1",
        "stylelint-config-standard-scss": "^13.1.0"
    },
